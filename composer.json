{
    "name": "cornyk/yii2-template",
    "description": "Yii2 Template",
    "require": {
        "php": ">=7.4.0",
        "yiisoft/yii2": "~2.0.45",
        "yidas/yii2-composer-bower-skip": "^2.0",
        "yiithings/yii2-dotenv": "^1.0",
        "yiisoft/yii2-redis": "^2.0",
        "smarty/smarty": "^3.1",
        "yiisoft/yii2-smarty": "^2.0",
        "pda/pheanstalk": "^3.2",
        "yiisoft/yii2-queue": "^2.3",
        "guzzlehttp/guzzle": "^7.5",
        "ext-json": "*"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.1.0",
        "yiisoft/yii2-gii": "~2.2.0",
        "yiisoft/yii2-faker": "~2.0.0",
        "phpunit/phpunit": "~9.5.0"
    },
    "autoload": {
        "psr-4": {
            "app\\": "src/",
            "components\\": "bootstrap/components/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer/"
        }
    }
}
